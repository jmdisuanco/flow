{
  "name": "@jmdisuanco/flow",
  "version": "1.3.1",
  "description": "A lightweight, functional approach to async pipelines with built-in Zod validation",
  "main": "lib/index.js",
  "publishConfig": {
    "access": "public"
  },
  "module": "lib/index.esm.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "src",
    "examples",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run check && npm run type-check && rollup -c",
    "build:watch": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "biome lint .",
    "format": "biome format . --write",
    "check": "biome check . --write",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf lib coverage",
    "prepublishOnly": "npm run clean && npm run build && npm test",
    "docs": "jsdoc src -r -d docs",
    "release": "bumpp && npm publish --registry=https://registry.npmjs.org/"
  },
  "keywords": [
    "flow-based-programming",
    "pipe",
    "react-flow",
    "dataflow",
    "orchestration",
    "parallel",
    "validation",
    "zod",
    "schema",
    "typescript"
  ],
  "author": "John Martin R. Disuanco <jmdisuanco@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jmdisuanco/flow.git"
  },
  "dependencies": {
    "bumpp": "10.3.2",
    "zod": "4.0.16"
  },
  "devDependencies": {
    "@babel/core": "7.26.0",
    "@babel/preset-env": "7.26.0",
    "@biomejs/biome": "2.3.10",
    "@rollup/plugin-babel": "6.0.4",
    "@rollup/plugin-node-resolve": "16.0.1",
    "@rollup/plugin-terser": "0.4.4",
    "@rollup/plugin-typescript": "12.1.2",
    "@types/jest": "30.0.0",
    "@types/node": "22.10.6",
    "jest": "30.0.5",
    "jsdoc": "4.0.4",
    "rollup": "4.30.1",
    "ts-jest": "29.2.5",
    "tslib": "2.8.1",
    "typescript": "5.7.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}